{
  "name": "picture-collage-creator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "check:coverage": "vitest run --coverage",
    "check:duplication": "jscpd src --config config/jscpd.json --threshold 75",
    "check:complexity": "eslint . --config eslint.complexity.config.js",
    "check:mutation": "stryker run && node scripts/check-mutants.cjs",
    "check:offline": "node scripts/check-offline.cjs",
    "check:qa": "eslint . --config eslint.qa.config.js",
    "check:spelling": "cspell \"**/*\" --config .cspell.json --no-progress",
    "check:all": "npm run lint && npm run check:complexity && npm run check:duplication && npm run check:offline && npm run check:coverage && npm run check:mutation && npm run check:spelling"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.25",
    "konva": "^10.0.12",
    "lucide-react": "^0.556.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-konva": "^19.2.1",
    "tailwind-merge": "^3.4.0",
    "use-image": "^1.1.4",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.57.0",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "cspell": "^9.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-complexity": "^1.0.2",
    "eslint-plugin-jsdoc": "^61.4.2",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-yml": "^1.19.0",
    "globals": "^16.5.0",
    "jscpd": "^4.0.5",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
